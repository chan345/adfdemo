											"typeConversionSettings": {
												"allowDataTruncation": true,
												"treatBooleanAsNumber": false
											}
										}
									},
									"inputs": [
										{
											"referenceName": "wiley_input",
											"type": "DatasetReference"
										}
									],
									"outputs": [
										{
											"referenceName": "outfile",
											"type": "DatasetReference"
										}
									]
								}
							]
						},
						{
							"value": "out2",
							"activities": [
								{
									"name": "Set variable2",
									"type": "SetVariable",
									"dependsOn": [],
									"userProperties": [],
									"typeProperties": {
										"variableName": "s2",
										"value": "'s2 sucess'"
									}
								}
							]
						}
					]
				}
			},
			{
				"name": "Set variable1",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "v",
					"value": "'out1'"
				}
			}
		],
		"variables": {
			"v": {
				"type": "String",
				"defaultValue": "out1"
			},
			"s1": {
				"type": "String"
			},
			"s2": {
				"type": "String"

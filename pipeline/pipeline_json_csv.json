{
	"name": "pipeline_json_csv",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['menu']['id']"
								},
								"sink": {
									"ordinal": 1
								}
							},
							{
								"source": {
									"path": "$['menu']['value']"
								},
								"sink": {
									"ordinal": 2
								}
							},
							{
								"source": {
									"path": "$['menu']['popup']['menuitem'][0]['value']"
								},
								"sink": {
									"ordinal": 3
								}
							},
							{
								"source": {
									"path": "$['menu']['popup']['menuitem'][0]['onclick']"
								},
								"sink": {
									"ordinal": 4
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "json_example",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "country_file",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}
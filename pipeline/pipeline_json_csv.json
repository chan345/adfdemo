{
	"name": "pipeline_json_csv",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['glossary']['title']"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['title']"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['GlossList']['GlossEntry']['ID']"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['GlossList']['GlossEntry']['SortAs']"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['GlossList']['GlossEntry']['GlossTerm']"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['GlossList']['GlossEntry']['Acronym']"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['GlossList']['GlossEntry']['Abbrev']"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['GlossList']['GlossEntry']['GlossDef']['para']"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['GlossList']['GlossEntry']['GlossDef']['GlossSeeAlso'][0]"
								}
							},
							{
								"source": {
									"path": "$['glossary']['GlossDiv']['GlossList']['GlossEntry']['GlossSee']"
								}
							}
						],
						"collectionReference": "$['menu']['popup']['menuitem']",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "json_example",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "country_file",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}